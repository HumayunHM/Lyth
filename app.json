{
   "expo": {
      "name": "Lyth",
      "slug": "Lyth",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/images/icon.png",
      "scheme": "lyth",
      "userInterfaceStyle": "automatic",
      "newArchEnabled": true,
      "ios": {
         "supportsTablet": true
      },
      "android": {
         "adaptiveIcon": {
            "backgroundColor": "#E6F4FE",
            "foregroundImage": "./assets/images/icon.png"
         },
         "edgeToEdgeEnabled": true,
         "predictiveBackGestureEnabled": false,
         "package": "com.humayunhm.Lyth"
      },
      "web": {
         "output": "static",
         "favicon": "./assets/images/icon.png"
      },
      "plugins": [
         "expo-router",
         [
            "expo-splash-screen",
            {
               "image": "./assets/images/splash-icon.png",
               "imageWidth": 200,
               "resizeMode": "contain",
               "backgroundColor": "#ffffff",
               "dark": {
                  "backgroundColor": "#000000"
               }
            }
         ]
      ],
      "experiments": {
         "typedRoutes": true,
         "reactCompiler": true
      },
      "extra": {
         "router": {},
         "eas": {
            "projectId": "6e15336b-ce77-4752-ba57-fa2339b721ab"
         }
      }
   }
}
